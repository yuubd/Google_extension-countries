{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "debugger"],
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "_comment": "css: [contentscript.css]",
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "React Chrome Extension Starter"
  },
  "icons": {
    "16": "assets/EduGlobe-logos_16x16.png",
    "32": "assets/EduGlobe-logos_32x32.png",
    "48": "assets/EduGlobe-logos_48x48.png",
    "128": "assets/EduGlobe-logos_128x128.png"
  },
  "web_accessible_resources": ["assets/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
